charcoal = #36454f
bannerBlue = rgb(206,232,247)

// Right Arrow Animations
@-webkit-keyframes middle 
    from 
    	height 200px; width 200px; left 200px; top 0px;
    	
    to 
    	height 100px; width 100px; left 500px; top 100px;
    	
@-webkit-keyframes right 
    from 
    	left 500px; 
    	
    to 
    	left 0px; 
    	
@-webkit-keyframes left 
    from 
    	height 100px; width 100px; left 0; top 100px; z-index: 0;
    	
    to 
    	height 200px; width 200px; left 200px; top 0; z-index: 1;
// Left Arrow Animations
    	// coming from middle
@-webkit-keyframes middley 
    from 
    	height 200px; width 200px; left 200px; top 0px;
    	
    to 
    	height 100px; width 100px; left 0px; top 100px;
    	
    // coming from left
@-webkit-keyframes lefty
    from 
    	left 0px;
    	
    to 
    	left 500px; 
    	
    //coming from right	
@-webkit-keyframes righty 
    from 
    	height 100px; width 100px; left 500; top 100px; z-index: 0;
    	
    to 
    	height 200px; width 200px; left 200px; top 0; z-index: 1;

.col-xs-offset-3
	margin-bottom 80px

.shadow
	position relative
	top 100%
	width 90%
	height 10px
	box-shadow 0 50px 30px #545454
	margin 0 auto
	border-radius 100px

#left, #middle, #right, #lefty, #righty, #middley
	position absolute
	margin-top 20px
	display inline-block
	border-radius 15px
	opacity .8
	
.portPiece
	position relative
	width 100%
	border-radius 5px
	
#left
	height 100px
	width 100px
	top 100px
	left 0px
	animation right 1s 1

.pointer
	pointer-events none
	
#middle .pointer, #middley .pointer
	pointer-events auto
	
.ani img
	border 1px solid charcoal
	padding 2px
	
.ani img:hover
	border 5px solid charcoal
	
#lefty
	height 100px
	width 100px
	top 100px
	left 0px
	animation middley 1s 1
	
#middle
	height 200px
	width 200px
	left 200px
	animation left 1s 1
	
#middley
	height 200px
	width 200px
	left 200px
	animation righty 1s 1
	
#right
	height 100px
	width 100px
	left 500px
	top 100px
	animation middle 1s 1
	
#righty
	height 100px
	width 100px
	left 500px
	top 100px
	animation lefty 1s 1

