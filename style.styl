.slidedown {
    position: absolute;
    top: 25%;
    left: 0;
    width: 100%;
    height: 100%;
}
.slidedown.ng-enter,
.slidedown.ng-leave {
    -webkit-transition: all 1s ease;
    transition: all 1s ease;
}
.slidedown.ng-enter {
    top: 100%;
}
.slidedown.ng-enter-active {
    top: 25%;
}
.slidedown.ng-leave {
    top: 25%;
}
.slidedown.ng-leave-active {
    top: 100%;
}

