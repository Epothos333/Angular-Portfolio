charcoal = #36454f
bannerBlue = rgb(206,232,247)

a 
	text-decoration none

.banner
	background-color bannerBlue
	border-bottom 6px solid charcoal
	border-radius 5px
	margin 0

.centerMe
	margin 0 auto

body
	font-family audiowide

.container-fluid
	padding-left 0
	padding-right 0
	margin-right 0
